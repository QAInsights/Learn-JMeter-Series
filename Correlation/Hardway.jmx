            <stringProp name="parameters"></stringProp>
            <stringProp name="script">response = prev.getResponseDataAsString()
println &quot;*********START***********&quot;
def extractTitle = /&lt;title&gt;(.+?)&lt;\/title&gt;/
def matcher = response =~ extractTitle

println &quot;Matched &quot; + matcher.find()
//log.info &apos;Named group &apos; + matcher.group(&apos;pageTitle&apos;)
//println &quot;Size &quot; + matcher.size()
//println &quot;All &quot; + matcher.findAll()
//println &quot;Extracted title &quot; + matcher.findAll()[0][1]
//println &quot;Matched &quot; + matcher.find()

if (matcher.size() &gt;=1) {
    println &quot;Match found&quot;
    println matcher.findAll().getClass() // Get type
    println matcher.findAll()[0][1]
    vars.put(&quot;extractTitle&quot;,matcher.findAll()[0][1])
}

def extractProduct = /a href=&quot;\/jpetstore\/actions\/Catalog.action;jsessionid=(.+?)?viewCategory=&amp;amp;categoryId=(.+?)&quot;/
def matcherProduct = response =~ extractProduct

println &quot;Matched &quot; + matcherProduct.find()
//println &quot;Size &quot; + matcher.size()
//println &quot;All &quot; + matcher.findAll()
//println &quot;Extracted title &quot; + matcher.findAll()[0][1]
//println &quot;Matched &quot; + matcher.find()

if (matcherProduct.size() &gt;=1) {
    println &quot;Match found&quot;
    println matcherProduct.findAll() // Get type - ArrayList
    println matcherProduct.findAll()[0][2]
    vars.put(&quot;extractProduct&quot;,matcherProduct.findAll()[0][2])
}

</stringProp>
